# vsim work.tb_dffg 
# Start time: 10:24:10 on Feb 01,2024
# ** Note: (vsim-3812) Design is being optimized...
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.tb_dffg(behavior)#1
add wave -position insertpoint  \
sim:/tb_dffg/DUT/s_D \
sim:/tb_dffg/DUT/s_Q
add wave -position insertpoint sim:/tb_dffg/P_TB/*
# (vish-4014) No objects found matching 'sim:/tb_dffg/P_TB/*'.
add wave -position insertpoint sim:/tb_dffg/P_CLK/*
# (vish-4014) No objects found matching 'sim:/tb_dffg/P_CLK/*'.
