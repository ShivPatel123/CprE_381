sim:/tb_dffg/gCLK_HPER \
sim:/tb_dffg/s_CLK \
sim:/tb_dffg/s_RST \
sim:/tb_dffg/s_WE \
sim:/tb_dffg/s_D \
sim:/tb_dffg/cCLK_PER
run 200
run 200
quit -sim
